{
  "name" : "pathfinder",
  "description": "Pathfinder is a Caleydo Web application for the visual exploration of paths in large graphs.",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/pathfinder",
  "homepage": "http://www.caleydo.org/tools/pathfinder/",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "pathfinder_ccle": "*",
    "pathfinder_graph": "*",
    "caleydo_bootstrap_fontawesome": "*"
  },
  "caleydo": {
    "type": "app",
    "plugins": {
      "web": [
        {
          "type": "app",
          "name": "Pathfinder"
        }
      ]
    },
    "products":[{
      "name": "Pathfinder",
      "type": "python",
      "bundle": true,
      "plugins": [
        "pathfinder",
        "pathfinder_ccle",
        "pathfinder_graph"
      ]
    }],
    "dependencies": {
      "web": {
        "d3": "~3.5.5",
        "webcola": "~3.1.0",
        "jquery-ui": "~1.11.4",
        "graphlib": "~1.0.1",
        "dagre": "~0.7.1"
      }
    },

    "requirejs-config": {
      "paths": {
        "lodash": "${baseUrl}/lodash/dist/lodash",
        "graphlib": "${baseUrl}/graphlib/dist/graphlib.core",
        "dagre": "${baseUrl}/dagre/dist/dagre.core"
      },
      "shim": {
        "webcola": {
          "deps": ["d3"],
          "exports": "cola"
        },
        "graphlib": {
          "deps": ["lodash"],
          "exports": "graphlib"
        },
        "dagre": {
          "deps": ["lodash", "graphlib"],
          "exports": "dagre"
        }
      }
    }
  }
}
