{
  "name" : "pathfinder",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/pathfinder",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "pathfinder_ccle": "*",
    "pathfinder_graph": "*",
    "caleydo_bootstrap_fontawesome": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "app",
          "name": "PPI"
        }
      ]
    },
    "products":[{
      "name": "Pathfinder",
      "type": "python",
      "bundle": true,
      "plugins": [
        "pathfinder",
        "pathfinder_ccle",
        "pathfinder_graph"
      ]
    }],
    "dependencies": {
      "web": {
        "d3": "~3.5.5",
        "webcola": "~3.1.0",
        "jquery-ui": "~1.11.4",
        "graphlib": "~1.0.1",
        "dagre": "~0.7.1",
        "dagre-d3": "~0.4.3"
      }
    },

    "requirejs-config": {
      "paths": {
        "lodash": "${baseUrl}/lodash/dist/lodash",
        "graphlib": "${baseUrl}/graphlib/dist/graphlib.core",
        "dagre": "${baseUrl}/dagre/dist/dagre.core",
        "dagre-d3" : "${baseUrl}/dagre-d3/dist/dagre-d3.core"
      },
      "shim": {
        "webcola": {
          "deps": ["d3"],
          "exports": "cola"
        },
        "graphlib": {
          "deps": ["lodash"],
          "exports": "graphlib"
        },
        "dagre": {
          "deps": ["lodash", "graphlib"],
          "exports": "dagre"
        },
        "dagre-d3": {
          "deps": ["d3","lodash", "graphlib", "dagre"],
          "exports": "dagre-d3"
        }
      }
    }
  }
}
